{
    "extends": "tslint:latest",
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-submodule-imports": false,
        "no-console": false,
        "max-line-length": false,
        "no-bitwise": [
            true
        ],
        "object-literal-sort-keys": false
    }
}